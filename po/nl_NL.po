#: ../uicache.m:106
#, c-format
msgid "Update iOS registered applications and optionally restart SpringBoard\n\n"
msgstr "Update iOS geregistreerde applicaties en optioneel herstart SpringBoard\n\n"

#: ../uicache.m:108
#, c-format
msgid "  -a, --all                Update all system and internal applications\n"
msgstr "  -a --all                 Werk alle systeem en interne applicaties bij\n"

#: ../uicache.m:109
#, c-format
msgid "  -f, --force              Force -a to reregister all Applications\n"
"                              and modify App Store apps\n"
msgstr "  -f, --force               Forceer -a om alle Applicaties te herregistreren\n"
"                              en wijzig App Store apps\n"

#: ../uicache.m:111
#, c-format
msgid "  -p, --path <path>        Update application bundle at the specified path\n"
msgstr "  -p, --path <pad>         Werk de applicatiebundel bij op het opgegeven pad\n"

