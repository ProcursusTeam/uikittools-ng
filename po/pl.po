msgstr "limit czasu wynosi %s: %s"

#: ../uicache.m:104
#, c-format
msgid "Usage: %s [-afhlr] [-i id] [-p path] [-u path]\n"
"Modified work Copyright (C) 2021, Procursus Team. All Rights Reserved.\n\n"
msgstr "Składnia: %s [-afhlr] [-i id] [-p ścieżka] [-u ścieżka]\n"
"Zmodyfikowana praca Copyright (C) 2021, Zespół Procursus. Wszystkie prawa zastrzeżone.\n\n"

#: ../uicache.m:106
#, c-format
msgid "Update iOS registered applications and optionally restart SpringBoard\n\n"
msgstr "Aktualizuj zarejestrowane aplikacje iOS i opcjonalnie zrestartuj SpringBoard\n\n"

#: ../uicache.m:108
#, c-format
msgid "  -a, --all                Update all system and internal applications\n"
msgstr "  -a, --all aktualizuj wszystkie aplikacje systemowe i wewnętrzne\n"

#: ../uicache.m:109
#, c-format
msgid "  -f, --force              Force -a to reregister all Applications\n"
"                              and modify App Store apps\n"
msgstr "  -f, --force Wymuś -a aby zarejestrować wszystkie aplikacje\n"
"                              i zmodyfikować aplikacje App Store\n"

#: ../uicache.m:111
#, c-format
msgid "  -p, --path <path>        Update application bundle at the specified path\n"
msgstr "  -p, --path <path>        Aktualizuj pakiet aplikacji w określonej ścieżce\n"

#: ../uicache.m:112
#, c-format
msgid "  -u, --unregister <path>  Unregister application bundle at the specified path\n"
msgstr "  -p, --path <path> Aktualizuj pakiet aplikacji w określonej ścieżce\n"

#: ../uicache.m:113
#, c-format
msgid "  -r, --respring           Restart SpringBoard and backboardd after\n"
"                              updating applications\n"
msgstr "  -r, --respring Restart SpringBoard i backboardd po\n"
"                              aktualizacji aplikacji\n"

#: ../uicache.m:115
#, c-format
msgid "  -l, --list               List the bundle ids of installed apps\n"
msgstr "  -l, --list Lista identyfikatorów pakietu zainstalowanych aplikacji\n"

#: ../uicache.m:116
#, c-format
msgid "  -i, --info <bundleid>    Give information about given bundle id\n"
msgstr "  -i, --info <bundleid>    Podaj informacje o podanym identyfikatorze pakietu\n"

#: ../uicache.m:117
#, c-format
msgid "  -h, --help               Give this help list.\n\n"
msgstr "  -?, --help               Wyświetl pomoc i zakończ działanie.\n\n"

